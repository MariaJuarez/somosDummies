	<queryString>
		<![CDATA[SELECT
     proyecto_empleado.`tareas_proyecto` AS proyecto_empleado_tareas_proyecto,
     empleado.`apellidos` AS empleado_apellidos,
     empleado.`nombres` AS empleado_nombres,
     proyecto.`descripcion` AS proyecto_descripcion,
     proyecto_empleado.`fecha_fin` AS proyecto_empleado_fecha_fin,
     proyecto_empleado.`fecha_inicio` AS proyecto_empleado_fecha_inicio,
     cliente.`descripcion_cliente` AS cliente_descripcion_cliente,
     proyecto_empleado.`id_empleado` AS proyecto_empleado_id_empleado,
     proyecto_empleado.`id_proyecto` AS proyecto_empleado_id_proyecto,
     proyecto_empleado.`id_proyecto_empleado` AS proyecto_empleado_id_proyecto_empleado,
     cargo.`descripcion` AS cargo_descripcion
FROM
     `proyecto_empleado` proyecto_empleado INNER JOIN `cargo` cargo ON proyecto_empleado.`id_cargo` = cargo.`id_cargo`
     INNER JOIN `empleado` empleado ON proyecto_empleado.`id_empleado` = empleado.`id_empleado`
     INNER JOIN `proyecto` proyecto ON proyecto_empleado.`id_proyecto` = proyecto.`id_proyecto`
     INNER JOIN `cliente` cliente ON proyecto.`id_cliente` = cliente.`id_cliente`]]>
	</queryString>
