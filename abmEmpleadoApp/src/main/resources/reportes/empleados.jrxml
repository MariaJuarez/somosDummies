	<queryString>
		<![CDATA[SELECT
     empleado.`id_empleado` AS empleado_id_empleado,
     empleado.`apellidos` AS empleado_apellidos,
     empleado.`cuil` AS empleado_cuil,
     empleado.`domicilio` AS empleado_domicilio,
     empleado.`email` AS empleado_email,
     empleado.`legajo` AS empleado_legajo,
     empleado.`nombres` AS empleado_nombres,
     empleado.`telefono` AS empleado_telefono,
     area_A.`descripcion` AS area_A_descripcion,
     perfil.`abreviatura_perfil` AS perfil_abreviatura_perfil,
     perfil.`descripcion` AS perfil_descripcion
FROM
     `empleado` empleado INNER JOIN `area` area_A ON empleado.`id_centro_costo` = area_A.`id_centro_costo`
     INNER JOIN `perfil` perfil ON empleado.`id_perfil` = perfil.`id_perfil` ORDER BY empleado.`id_empleado`]]>
	</queryString>
