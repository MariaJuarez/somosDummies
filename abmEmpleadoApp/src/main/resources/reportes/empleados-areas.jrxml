	<queryString>
		<![CDATA[SELECT
     area.`descripcion` AS area_descripcion,
     empleado.`id_empleado` AS empleado_id_empleado,
     empleado.`apellidos` AS empleado_apellidos,
     empleado.`cuil` AS empleado_cuil,
     empleado.`domicilio` AS empleado_domicilio,
     empleado.`email` AS empleado_email,
     empleado.`nombres` AS empleado_nombres,
     empleado.`telefono` AS empleado_telefono,
     perfil.`descripcion` AS perfil_descripcion
FROM
     `empleado` empleado INNER JOIN `area` area ON empleado.`id_centro_costo` = area.`id_centro_costo`
     INNER JOIN `perfil` perfil ON empleado.`id_perfil` = perfil.`id_perfil`]]>
